
  widget {
    timeseries_definition {
      request {
        q            = "avg:aws.rds.read_iops{$engine} by {dbinstanceidentifier}.as_rate()+avg:aws.rds.write_iops{engine:mysql} by {dbinstanceidentifier}.as_rate()"
        display_type = "line"
        style {
          palette    = "blue"
          line_type  = "solid"
          line_width = "thin"
        }
      }
      yaxis {
        scale        = "linear"
        min          = "auto"
        max          = "auto"
        include_zero = true
        label        = ""
      }
      title_align = "center"
      title_size  = 13
      title       = "Total I/O"
    }
  }
